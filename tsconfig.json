{
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "rootDir": "./",
    "outDir": "./prod",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
  }
}
